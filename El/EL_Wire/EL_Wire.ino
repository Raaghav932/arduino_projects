void setup() {
  pinMode(3, OUTPUT);  // channel B 
  pinMode(4, OUTPUT);  // channel C
  pinMode(5, OUTPUT);  // channel D  
  pinMode(6, OUTPUT);  // channel E
  pinMode(7, OUTPUT);  // channel F
  pinMode(8, OUTPUT);  // channel G
  pinMode(9, OUTPUT);  // channel H


}
void loop() {
  pinMode(3, HIGH);  // channel B ON
  pinMode(4, HIGH);  // channel C ON
  pinMode(5, HIGH);  // channel D ON
  pinMode(6, HIGH);  // channel E ON
  pinMode(7, HIGH);  // channel F ON
  pinMode(8, HIGH);  // channel G ON
  pinMode(9, HIGH);  // channel H ON

}




